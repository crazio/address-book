{
    "_version": "1.8.0",
    "sap.app": {
        "id": "person",
        "type": "application",
        "title": "{{appTitle}}",
        "description": "Sample Application",
        "i18n": "i18n/i18n.properties",
        "dataSources": {
            "PersonService": {
                "uri": "/person/",
                "type": "OData",
                "settings": {
                    "odataVersion": "4.0"
                }
            }
        },
        "-sourceTemplate": {
            "id": "ui5template.basicSAPUI5ApplicationProject",
            "-id": "ui5template.smartTemplate",
            "-version": "1.40.12"
        }
    },
    "sap.ui5": {
        "dependencies": {
            "libs": {
                "sap.fe": {}
            }
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "uri": "i18n/i18n.properties"
            },
            "": {
                "dataSource": "PersonService",
                "settings": {
                    "synchronizationMode": "None",
                    "operationMode": "Server",
                    "autoExpandSelect": true,
                    "earlyRequests": true,
                    "groupProperties": {
                        "default": {
                            "submit": "Auto"
                        }
                    }
                }
            }
        },
        "routing": {
            "routes": [
                {
                    "pattern": ":?query:",
                    "name": "PersonsList",
                    "target": "PersonsList"
                },
                {
                    "pattern": "PersonAttributes({key}):?query:",
                    "name": "PersonsDetails",
                    "target": "PersonsDetails"
                }
            ],
            "targets": {
                "PersonsList": {
                    "type": "Component",
                    "id": "PersonsList",
                    "name": "sap.fe.templates.ListReport",
                    "options": {
                        "settings": {
                            "entitySet": "PersonAttributes",
                            "navigation": {
                                "Persons": {
                                    "detail": {
                                        "route": "PersonsDetails"
                                    }
                                }
                            }
                        }
                    }
                },
                "PersonsDetails": {
                    "type": "Component",
                    "id": "PersonsDetailsList",
                    "name": "sap.fe.templates.ObjectPage",
                    "options": {
                        "settings": {
                            "entitySet": "PersonAttributes"
                        }
                    }
                }
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "fullWidth": false
    },
    "sap.fiori": {
        "registrationIds": [],
        "archeType": "transactional"
    }
    
}